#include <stdio.h>

int main()
{
	//Задаем точки
	int A, B, C;
	printf_s("A: "); //Вывод надписи
	scanf_s("%d", &A); //Ввод информации
	printf_s("B: "); //Вывод надписи
	scanf_s("%d", &B); //Ввод информации
	printf_s("C: "); //Вывод надписи
	scanf_s("%d", &C); //Ввод информации
	//Вычисляем длину отрезков
	int AC = C - A;
	int BC = B - C;
	int sum = AC * BC; //Умножаем длины отрезков
	printf_s("%i", sum); //Вывод
	return 0; //Конец
}
