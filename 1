#include <stdio.h>
#include <Math.h>

int main()
{
	//Задаем координаты точек
	float x1, x2, y1, y2;
	printf_s("x1: "); //Вывод надписи
	scanf_s("%f", &x1); //Ввод информации
	printf_s("x2: "); //Вывод надписи
	scanf_s("%f", &x2); //Ввод информации
	printf_s("y1: "); //Вывод надписи
	scanf_s("%f", &y1); //Ввод информации
	printf_s("y2: "); //Вывод надписи
	scanf_s("%f", &y2); //Ввод информации
	float length;
	length = sqrt((x2 - x1) * (x2 - x1) + (y2 - y1) * (y2 - y1)); //Вычисляем расстояние между точками
	printf_s("%f", length); //Вывод
	return 0; //Конец
}
